            "values": [
                "true",
                "false"
            ]
        },
        {
            "name": "PULLDOWN",
            "description": "enables or disables I/O pulldown resistor of nominal 50kOhm",
            "values": [
                "true",
                "false"
            ]
        },
        {
            "name": "KEEPER",
            "description": " enables or disables I/O keeper option",
