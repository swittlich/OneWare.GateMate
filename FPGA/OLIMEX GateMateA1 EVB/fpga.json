            ]
        },
        {
            "name": "FF_IBF",
            "description": "enables or disables placing of FF in input buffer, if possible",
            "values": [
                "true",
                "false"
            ]
        },
        {
            "name": "FF_OBF",
            "description": "enables or disables placing of FF in output buffer, if possible",
            "values": [
                "true",
                "false"
            ]
        },
        {
            "name": "LVDS_BOOST",
